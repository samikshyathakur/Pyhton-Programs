   "source": [
    "#Approach 1:\n",
    "# 1) Find reverse of the string\n",
    "# 2) Check if reverse and original are same or not\n",
    "\n",
    "s = input(\"Enter a string:\")\n",
    "revstr = (s[::-1])\n",
    "\n",
    "if revstr == s:\n",
    "    print(\"Palindrome\")\n",
    "else:\n",
    "    print(\"Not Palindrome\")"
   ]
